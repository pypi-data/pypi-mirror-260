$PROBLEM EFFECTCOMP_LINEAR
$DATA ../.datasets/input_model.csv IGNORE=@
$INPUT ID TIME AMT WGT APGR DV DVID
$SUBROUTINE ADVAN13 TOL=9

$MODEL COMPARTMENT=(CENTRAL DEFDOSE) COMPARTMENT=(EFFECT)
$ABBR REPLACE ETA_SLOPE=ETA(3)
$ABBR REPLACE ETA_B=ETA(4)
$PK
SLOPE = THETA(5)*(ETA_SLOPE + 1)
B = THETA(4)*EXP(ETA_B)
MET = THETA(3)
CL = THETA(1)*EXP(ETA(1))
VC = THETA(2)*EXP(ETA(2))
V=VC
KE0 = 1/MET

$DES
DADT(1) = -A(1)*CL/V
DADT(2) = A(1)*KE0/V - A(2)*KE0
$ERROR
CONC = A(1)/VC
Y = CONC + CONC*EPS(1)
E = B*(A(2)*SLOPE + 1)
Y_2 = E + E*EPS(2)

IF (DVID.EQ.1) THEN
    Y = Y
ELSE
    Y = Y_2
END IF
$THETA (0,0.00273872) FIX ; TVCL
$THETA (0,1.44718) FIX ; TVV
$THETA  (0,0.1) ; POP_MET
$THETA  (0,5.75005) ; POP_B
$THETA  (0,0.1) ; POP_SLOPE
$OMEGA 3.09626e-06 FIX  ; IVCL
$OMEGA 3.1128e-06 FIX  ; IVV
$OMEGA  0.09 ; IIV_SLOPE
$OMEGA  0.09 ; IIV_B
$SIGMA 0.00630754 FIX

$SIGMA  0.338363 ; sigma
$ESTIMATION METHOD=1 INTERACTION

$TABLE ID TIME CWRES NOAPPEND
       NOPRINT ONEHEADER FILE=pd_sdtab2
