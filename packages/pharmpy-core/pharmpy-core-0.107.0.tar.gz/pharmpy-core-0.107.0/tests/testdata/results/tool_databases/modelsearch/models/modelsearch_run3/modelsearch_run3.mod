$PROBLEM PERIPHERALS(1);LAGTIME()
$INPUT ID VISI XAT2=DROP DGRP DOSE FLAG=DROP ONO=DROP
       XIME=DROP NEUY SCR AGE SEX NYHA=DROP WT DROP ACE
       DIG DIU NUMB=DROP TAD TIME VIDD=DROP CLCR AMT SS II DROP
       CMT=DROP CONO=DROP DV EVID=DROP OVID=DROP
$DATA ../.datasets/input_model.csv IGNORE=@
$SUBROUTINES ADVAN4  TRANS4
$PK
MDT = THETA(6)*EXP(ETA(4))
VP1 = THETA(5)
QP1 = THETA(4)
CL = THETA(1) * EXP(ETA(1))
VC = THETA(2) * EXP(ETA(2))
MAT = THETA(3) * EXP(ETA(3))
KA = 1/MAT
V2 = VC
Q = QP1
V3 = VP1
ALAG1 = MDT
$ERROR
CONC = A(2)/VC
Y = CONC + CONC * EPS(1)
$ESTIMATION METHOD=0
$THETA (0, 24.0058, Inf) ; POP_CL
$THETA (0, 59.1147, Inf) ; POP_VC
$THETA (0, 0.842498, Inf) ; POP_MAT
$THETA  (0,47.8042,999999) ; POP_QP1
$THETA  (0,33.8517,999999) ; POP_VP1
$THETA  (0,0.125) ; POP_MDT
$OMEGA 0.470531; IIV_CL
$OMEGA 1.51525; IIV_VC
$OMEGA 0.205369; IIV_MAT
$OMEGA  0.01 ; IIV_MDT
$SIGMA 0.193439; RUV_PROP
$TABLE ID TIME DV CWRES CIPREDI NOAPPEND FILE=mytab_mox3
