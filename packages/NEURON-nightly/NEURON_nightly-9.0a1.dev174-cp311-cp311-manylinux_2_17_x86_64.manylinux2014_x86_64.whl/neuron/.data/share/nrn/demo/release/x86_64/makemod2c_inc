$(PWD)/cabpump.cpp: ../cabpump.mod $(NOCMODL)
	@printf " -> $(C_GREEN)NMODL$(C_RESET) $<\\n"
	(cd "..";  MODLUNIT=$(NRNUNITS) $(NOCMODL) "cabpump.mod" -o "/root/nrn/build/cmake_install/share/nrn/demo/release/x86_64" )

./cabpump.o: $(PWD)/cabpump.cpp
	@printf " -> $(C_GREEN)Compiling$(C_RESET) $<\\n"
	$(CXXCOMPILE) -I".." $(INCLUDES) -fPIC -c $< -o $@

